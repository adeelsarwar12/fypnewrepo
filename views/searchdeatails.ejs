     <!-- Cards -->
     <div class="row">
         <% for(let i = 0; i < campgrounds.length; i++) { %>
         <div class="card" style="width: 18rem; margin-left: 50px; margin-top: 60px;">
             <img class="card-img-top" src="/<%=campgrounds[i].photo3 %> " alt="Card image cap">
             <div class="card-body">
                 <h5 class="card-title"><%=campgrounds[i].propertyType %></h5>
                 <label>Price</label>
                 <p class="card-text"><%=campgrounds[i].price %>.</p>
                 <label>Address</label>
                 <p class="card-text"><%=campgrounds[i].address %>.</p>
                 <a href="/property/detailpage/<%=campgrounds[i].id%>" class="btn btn-primary">Details</a>
             </div>
         </div>
         <% } %>

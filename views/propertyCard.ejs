<body>

    <div class="super_container">

        <!-- Header -->

        <header class="header">
            <div class="header_content d-flex flex-row align-items-center justify-content-start"
                style="background: rgb(43, 43, 43)">
                <div class="logo"><a href="#">Pak Hostel</a></div>
                <div class="ml-auto d-flex flex-row align-items-center justify-content-start">
                    <nav class="main_nav">
                        <ul class="d-flex flex-row align-items-start justify-content-start">
                            <li><a href="/">Home</a></li>
                            <li><a href="/aboutus">About us</a></li>
                            <li class="active"><a href="/hostels">Hostels</a></li>
                            <li><a href="/api/guesthouse">Guest House</a></li>
                            <li><a href="/contact/contact">Contact</a></li>
                        </ul>
                    </nav>
                    <div class="book_button"><a href="/api/signup">Register Hostel</a></div>
                    <div class="book_button"><a href="/api/login">Login</a></div>

                    <!-- Hamburger Menu -->
                    <div class="hamburger"><i class="fa fa-bars" aria-hidden="true"></i></div>
                </div>
            </div>
        </header>

        <!-- Menu -->

        <div class="menu trans_400 d-flex flex-column align-items-end justify-content-start">
            <div class="menu_close"><i class="fa fa-times" aria-hidden="true"></i></div>
            <div class="menu_content">
                <nav class="menu_nav text-right">
                    <ul>
                        <li><a href="/">Home</a></li>
                        <li><a href="/aboutus">About us</a></li>
                        <li><a href="/hostels">Hostels</a></li>
                        <li><a href="/api/guesthouse">Guest House</a></li>
                        <li><a href="/contact/contact">Contact</a></li>
                    </ul>
                </nav>
            </div>
            <div class="menu_extra">
                <div class="menu_book text-right"><a href="/api/signup">Register</a></div>
                <div class="menu_book text-right"><a href="/api/login">Login</a></div>

            </div>
        </div>

        <!-- Home -->


        <!-- wlcome -->
        <br /><br /><br /><br /><br /><br /><br />

        <!-- Cards -->
        <div class="row">
            <% for(let i = 0; i < campgrounds.length; i++) { %>
            <div class="card" style="width: 18rem; margin-left: 50px; margin-top: 60px;">
                <img class="card-img-top" src="/<%=campgrounds[i].photo1 %> " alt="Card image cap">
                <div class="card-body">
                    <h5 class="card-title"><%=campgrounds[i].hostelName %></h5>
                    <label>Address</label>
                    <p class="card-text"><%=campgrounds[i].address %>.</p>
                    <a href="/api/user/<%= campgrounds[i].adminId %>" class="btn btn-primary">Details</a>
                </div>
            </div>
            <% } %>
        </div>
        <!-- Footer -->

        <footer class="footer">
            <div class="footer_content">
                <div class="container">
                    <div class="row">
                        <div class="col">
                            <div class="footer_logo_container text-center">
                                <div class="footer_logo">
                                    <a href="#"></a>
                                    <div>PakHostels</div>
                                    <div>2019</div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row footer_row">

                        <!-- Address -->
                        <div class="col-lg-3">
                            <div class="footer_title">Our Address</div>
                            <div class="footer_list">
                                <ul>
                                    <li>Bahria Town</li>
                                    <li>Phase 8 Rwp</li>
                                    <li>Pakistan</li>
                                </ul>
                            </div>
                        </div>

                        <!-- Reservations -->
                        <div class="col-lg-3">
                            <div class="footer_title">Reservations</div>
                            <div class="footer_list">
                                <ul>
                                    <li>Tel: 0092-342-5495747</li>
                                    <li>Fax: ---------------</li>
                                    <li>Email:p.pakistan47@gmail.com</li>
                                </ul>
                            </div>
                        </div>

                        <!-- Newsletter -->
                        <div class="col-lg-3">
                            <div class="footer_title">Newsletter</div>
                            <div class="newsletter_container">
                                <form action="#" class="newsletter_form" id="newsletter_form">
                                    <input type="email" class="newsletter_input" placeholder="Your email address"
                                        required="required">
                                    <button class="newsletter_button">Subscribe</button>
                                </form>
                            </div>
                        </div>

                        <!-- Footer images -->
                        <div class="col-lg-3">
                            <div
                                class="certificates d-flex flex-row align-items-start justify-content-lg-between justify-content-start flex-lg-nowrap flex-wrap">
                                <div class="cert"><img src="/images/cert_1.png" alt=""></div>
                                <div class="cert"><img src="/images/cert_2.png" alt=""></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="copyright">
                <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                Copyright &copy;
                <script>document.write(new Date().getFullYear());</script> All rights reserved | <i
                    class="fa fa-heart-o" aria-hidden="true"></i><a href="https://www.facebook.com/muhammad.fazeel1"
                    target="_blank">K@ShMiRi</a>
                <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
            </div>
        </footer>
    </div>
